SET_UP_ANALIZER	PROC	NEAR
	PUSH	AX
	PUSH	BX
	PUSH	DS

	PUSH	BX
	PUSH	DS
	PUSH	AX
	XOR	AX,AX
	MOV	DS,AX
	MOV	AX,CS
	LEA	BX,INT_1
	CLI	
	XCHG	AX,DS:[1*4+2]
	XCHG	BX,DS:[1*4]
	STI
	MOV	CS:[INT_1_OFF_],BX
	MOV	CS:[INT_1_SEG_],AX
	POP	AX
	POP	DS
	POP	BX
	OR	CS:[AN_FLAGS_],10000000B
	MOV	CS:[ABS_ADRESS_L_BEGIN_],DX
	MOV	CS:[ABS_ADRESS_H_BEGIN_],CL
	MOV	CS:[LEAVE_ADRESS_OFF_],BX
	MOV	CS:[LEAVE_ADRESS_SEG_],DS
	LEA	SI,EXIT_SEQ_
	MOV	CS:[COMMAND_ADRESS_OFF_],SI
	PUSH	CS
	POP	DS
	MOV	CS:[COMMAND_ADRESS_seg_],DS
	MOV	AX,CS:[FUNCT_COD_]
	MOV	CS:[COMMAND_CHART_],AX
	POP	DS
	POP	BX
	POP	AX
	RET
INT_1_OFF_	DW	0
INT_1_SEG_	DW	0
EXIT_SEQ_:
	MOV	DI,AX
	RETF
FUNCT_COD_	DW	1110000000000000B
ENDP
RESTORE_ANALIZER	PROC	NEAR
	XOR	AX,AX
	MOV	DS,AX
	MOV	BX,CS:[INT_1_OFF_]
	MOV	AX,CS:[INT_1_SEG_]
	CLI	
	XCHG	AX,DS:[1*4+2]
	XCHG	BX,DS:[1*4]
	STI
	RET
ENDP
