PRINT_SYMBOL	PROC	NEAR
	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	PUSH	SI
	PUSH	DS
	MOV	CS:[X_A_],0
	MOV	CS:[Y_A_],0
	CALL	ZERO
	MOV	BX,1
	MOV	CX,1
	MOV	DL,[SCREEN_]
	CMP	DL,1
	JE	PRINT_SYMBOL_1
	ADD	CX,12
	CMP	DL,2
	JE	PRINT_SYMBOL_1
	ADD	BX,26
PRINT_SYMBOL_1:
	ADD	CX,[Y_]
	ADD	BX,[X_]
	MOV	[X_A_],0
	MOV	[Y_A_],0
	MOV	CS:[X_MAX_],1
	MOV	CS:[Y_MAX_],1
	INT	50
	MOV	AX,CX
	MOV	CL,3
	ROL	AX,CL
	ROL	BX,CL
	MOV	CX,8
	MOV	DX,CX
	XOR	SI,SI
	MOV	DS,CS:[LEAVE_BLOCK_]
	INT	88

        MOV	BX,CS:[X_]
        INC	BX
	MOV	CS:[X_],BX

	POP	DS
	POP	SI
	POP	DX
	POP	CX
	POP	BX
	POP	AX
	RET
ENDP
