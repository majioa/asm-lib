HEX2DECI	PROC	NEAR
	;DS:SI-ADRESS OF NUMBER
	;ES:DI-NEW NUMBER
	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	PUSH	DI
	MOV	BX,10
	XOR	CX,CX
	LODSW
HEX2DECI_1:
	XOR	DX,DX
	DIV	BX
	PUSH	DX
	INC	CX
	OR	AX,AX
	JNZ	HEX2DECI_1
	MOV	AL,CL
	STOSB
HEX2DECI_2:
	POP	AX
	STOSB
	LOOP	HEX2DECI_2
	POP	DI
	POP	DX
	POP	CX
	POP	BX
	POP	AX
	RET
ENDP
