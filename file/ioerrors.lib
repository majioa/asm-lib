PRINT_IO_ERRORS PROC
	PUSH	AX
	PUSH	SI
	PUSH	DS


	PUSH	CS
	POP	DS
	PUSH	AX
	PUSH	SI
	LEA	SI,FILE_
	DEC	AX
	DEC	AX
	JZ	PRINT_IO_ERROR_3
	LEA	SI,PATH_
	DEC	AX
	JNZ	PRINT_IO_ERROR_1
PRINT_IO_ERROR_3:
	CALL	WW
	POP	SI
	JMP	SHORT	PRINT_IO_ERROR_4
PRINT_IO_ERROR_1:
	POP	SI
	LEA	SI,ERROR_
PRINT_IO_ERROR_4:
	CALL	WW
	POP	AX
	LEA	SI,WRONG_NUMBER_OF_FUNCTION_
	DEC	AX
	JZ	PRINT_IO_ERROR_2
	LEA	SI,NOT_FOUND_
	DEC	AX
	JZ	PRINT_IO_ERROR_2
	DEC	AX
	JZ	PRINT_IO_ERROR_2
	LEA	SI,NO_MORE_DESCRIPTORS_
	DEC	AX
	JZ	PRINT_IO_ERROR_2
	LEA	SI,ACCESS_DENIED_
	DEC	AX
	JZ	PRINT_IO_ERROR_2
	LEA	SI,FILE_ERROR_
PRINT_IO_ERROR_2:
	CALL	WW
	POP	DS
	POP	SI
	POP	AX
	RET


FILE_	DB	'File ',0
PATH_	DB	'Path to file ',0
NOT_FOUND_	DB	' not found',0dh,0ah,0
ERROR_	DB	'Error:',0

WRONG_NUMBER_OF_FUNCTION_	DB	'Wrong number of function',0dh,0ah,0
NO_MORE_DESCRIPTORS_	DB	'No more descriptors',0dh,0ah,0
ACCESS_DENIED_	DB	'Access denied',0dh,0ah,0
WRONG_DESCRIPTOR_	DB	'Wrong descriptors',0dh,0ah,0
FILE_ERROR_	DB	'Unrecognized file error',0dh,0ah,0
ENDP